<?php

/**
 * @file
 * Default template to load the twitter Feeds.
 */

function twitter_tweets_theme() {
  return array(
    'twitter_tweets_tweet_listing' => array(
      'variables' => array('params' => array()),
    ),
  );
}

/*
 * CSS Library to load for the default template
 */

function twitter_tweets_page_attachments(array &$page) {
  

  $page['#attached']['library'][] = 'twitter_tweets/twitter_tweets.twitter_feeds';
}
