
    <div class="row" id="footer-menu-links">
      <div class="col-lg-6 col-md-6 col-sm-6" >
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12"><br><br></div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                {{ page.footer_first }}
            </div>
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6" >
          <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12">
                  {% if page.footer_social %}
                      {{ page.footer_social }}
                  {% else %}
                      {% include '@bootstrap_italia_components/social/social-footer.twig' %}
                  {% endif %}
              </div>
              {% if page.footer_second %}
                  <div class="col-lg-12 col-md-12 col-sm-12">
                      {{ page.footer_second }}
                  </div>
              {% endif %}
          </div>
      </div>
    </div>
    {% if (page.footer_third or page.footer_fourth) %}
        <div class="row">
          {% if page.footer_third %}
              <div class="col-lg-6 col-md-6 col-sm-6 ">
                  {{ page.footer_third }}
              </div>
          {% endif %}
          {% if page.footer_fourth  %}
              <div class="col-lg-6 col-md-6 col-sm-6">
                  {{ page.footer_fourth }}
              </div>
          {% endif %}
        </div>
    {% endif %}

